@model IEnumerable<string>

<a class="btn btn-block btn-outline-secondary" 
   asp-action="Index" 
   asp-controller="Home"
   asp-route-category="">Home</a>

@foreach (string category in Model)
{                           @* To see determine which button will appear *@
    <a class="btn btn-block @(category == ViewBag.SelectedType ? "btn-primary" : "btn-outline-secondary")"
       asp-action="Index" 
       asp-controller="Home"
       asp-route-category="@category"
       asp-route-page="1">
        @category
    </a>
}
